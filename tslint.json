{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "no-console": false,
        "no-consecutive-blank-lines": false,
        "eofline": false,
        "no-trailing-whitespace": false
    },
    "rulesDirectory": []
}