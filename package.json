{
  "name": "express-ts-starter",
  "version": "0.0.1",
  "description": "an express app starter that uses typescript",
  "main": "app.ts",
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint",
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --transpileOnly -- ./src/app.ts",
    "debug": "ts-node-dev --transpileOnly --inspect -- ./src/app.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "tslint"
  ],
  "repository": {
    "git": "https://github.com/mikecabana/express-ts-starter.git"
  },
  "author": "Michael Cabana <mikeycabana@gmail.com>",
  "contributors": [
    {
      "name": "Michael Cabana",
      "email": "mikeycabana@gmail.com",
      "url": "https://github.com/mikecabana"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.43",
    "@types/morgan": "^1.7.37",
    "@types/node": "^11.15.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1"
  }
}
